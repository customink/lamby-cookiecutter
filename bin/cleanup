#!/bin/sh
set -e

echo "== Cleaning Up Project =="

PROJECT_FOLDER="{{cookiecutter.project_name}}"
cd $PROJECT_FOLDER

rm -rf ./.git \
       ./config/puma \
       ./config/master.key \
       ./config/credentials.yml.enc \
       ./Gemfile.lock \
       ./node_modules \
       ./log/development.log \
       yarn.lock
